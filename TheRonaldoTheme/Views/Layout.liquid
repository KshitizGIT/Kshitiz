<!DOCTYPE html>
<html lang="{{Culture.Name}}">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>{% page_title Site.SiteName %}</title>
    
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css" >

    <link rel="stylesheet" href="/TheRonaldoTheme/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="/TheRonaldoTheme/css/animate.css">
    
    <link rel="stylesheet" href="/TheRonaldoTheme/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/TheRonaldoTheme/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="/TheRonaldoTheme/css/magnific-popup.css">

    <link rel="stylesheet" href="/TheRonaldoTheme/css/aos.css">

    <link rel="stylesheet" href="/TheRonaldoTheme/css/ionicons.min.css">
    
    <link rel="stylesheet" href="/TheRonaldoTheme/css/flaticon.css">
    <link rel="stylesheet" href="/TheRonaldoTheme/css/icomoon.css">
    <link rel="stylesheet" href="/TheRonaldoTheme/css/style.css">
  </head>
  <body data-spy="scroll" data-target=".site-navbar-target" data-offset="300" dir={{Culture.Dir}}>
	  
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar ftco-navbar-light site-navbar-target" id="ftco-navbar">
	    <div class="container">
            {% assign contents =  Content["alias:default"].Content %}
          {% assign owner = Site.Properties.Portfolio.Portfolio.Owner.Text | split: ' '| first  %}
	      <a class="navbar-brand" href="#"><span>{{ owner | split: '' | first}}</span> {% for l in owner  offset: 1 %} {{l}} {% endfor %}</a>
	      <button class="navbar-toggler js-fh5co-nav-toggle fh5co-nav-toggle" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
	        <span class="oi oi-menu"></span> Menu
	      </button>
		 {% shape "menu", alias: "alias:main-menu", cache_id: "main-menu", cache_tag: "alias:main-menu" %}
	    </div>
	  </nav>
	  {% render_section "Content" %}

      {% render_section "Footer", required: false %}
  

  <!-- loader -->
  <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>


  <script src="/TheRonaldoTheme/js/jquery.min.js"></script>
  <script src="/TheRonaldoTheme/js/jquery-migrate-3.0.1.min.js"></script>
  <script src="/TheRonaldoTheme/js/popper.min.js"></script>
  <script src="/TheRonaldoTheme/js/bootstrap.min.js"></script>
  <script src="/TheRonaldoTheme/js/jquery.easing.1.3.js"></script>
  <script src="/TheRonaldoTheme/js/jquery.waypoints.min.js"></script>
  <script src="/TheRonaldoTheme/js/jquery.stellar.min.js"></script>
  <script src="/TheRonaldoTheme/js/owl.carousel.min.js"></script>
  <script src="/TheRonaldoTheme/js/jquery.magnific-popup.min.js"></script>
  <script src="/TheRonaldoTheme/js/aos.js"></script>
  <script src="/TheRonaldoTheme/js/jquery.animateNumber.min.js"></script>
  <script src="/TheRonaldoTheme/js/scrollax.min.js"></script>
  
  <script src="/TheRonaldoTheme/js/main.js"></script>
{% resources type: "FootScript" %}    

  </body>
</html>